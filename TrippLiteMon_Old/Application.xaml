<Application x:Class="Application"
    x:Name="TrippLiteMon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:TrippLite="clr-namespace:TrippLite">

    <!--StartupUri="/TLControls;component/DesktopWindow.xaml"-->

    <!--StartupUri="View/PowerMon.xaml"-->

    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/TLControls;component/Resources.xaml"/>
                <ResourceDictionary>                   
                    <ImageSource x:Key="TrippLiteIcon">pack://application:,,,/TrippLiteMon;component/Resources/TrippLite.ico</ImageSource>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
